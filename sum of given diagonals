5.SUM OF THE DIAGONALS IN GIVEN ARRAY.
#include<stdio.h>
int main()
{
	int i,j,n,m;
	int major=0,minor=0;
	printf("enter the rows: ");
	scanf("%d",&n);
	printf("enter col: ");
	scanf("%d",&m);
int a[n][m];
printf("enter the elements:");
for(i=0;i<n;i++)
{
	for(j=0;j<m;j++)
	{
		scanf("%d",&a[i][j]);
	}
	}
	for(i=0;i<n;i++)
	{
	
	major+=a[i][i];
	minor+=a[i][n-i-1];
}

	printf("sum of major =  %d",major);
	printf("sum of minor = \n:Â %d",minor);

}
